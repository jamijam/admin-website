{
  "name": "opencerts-admin-ui",
  "version": "0.0.1",
  "scripts": {
    "dev": "next",
    "lint": "eslint . --ext .js --max-warnings 0",
    "lint:fix": "eslint . --ext .js --fix",
    "build": "next build",
    "build:static": "next build && next export",
    "start": "next build && next start",
    "cname": "echo 'admin.opencerts.io' > out/CNAME",
    "deploy": "yarn deploy:prebuilt && gh-pages --dotfiles --dist out",
    "deploy:prebuilt": "rm -rf out && yarn build:static && touch out/.nojekyll && yarn cname"
  },
  "dependencies": {
    "@ledgerhq/hw-transport-u2f": "^4.21.0",
    "@ledgerhq/web3-subprovider": "^4.23.0",
    "eslint-plugin-react": "^7.7.0",
    "lodash": "^4.17.5",
    "next": "^10.0.6",
    "next-redux-wrapper": "^1.3.5",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.1",
    "react-tabs": "^2.2.1",
    "redux": "^3.6.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-saga": "^0.16.0",
    "truffle-contract": "^3.0.4",
    "web3": "^1.0.0-beta.33",
    "web3-provider-engine": "14.0.6"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.10.2",
    "webpack": "^4.0.1"
  }
}
